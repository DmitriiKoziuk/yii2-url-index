{
  "name": "dmitriikoziuk/yii2-url-index",
  "description": "Yii2 url index.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "url index"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitrii Koziuk",
      "email": "dmitriikoziuk@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.0.0",
    "dmitriikoziuk/yii2-base": "~0.3.0",
    "dmitriikoziuk/yii2-module-manager": "~0.3.0",
    "dmitriikoziuk/yii2-config-manager": "~0.3.0",
    "dmitriikoziuk/yii2-web-backend-lock": "~0.1.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "codeception/module-phpbrowser": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "DmitriiKoziuk\\yii2UrlIndex\\": "src",
      "DmitriiKoziuk\\yii2UrlIndex\\tests\\": "tests"
    }
  },
  "extra": {
    "bootstrap": "DmitriiKoziuk\\yii2UrlIndex\\Bootstrap"
  }
}
